
# Online Python - IDE, Editor, Compiler, Interpreter
def minimize(a,b,c,d):
    if(a>b):
        if(b>c):
            if(c>d):
                return d
            else:
                return c
        else:
            if(b>d):
                return d
            else:
                return b
    else:
        if(a>c):
            if(c>d):
                return d
            else:
                return c
        else:
            if(a>d):
                return d
            else:
                return a

a = int(input('Enter 1st number: '))
b = int(input('Enter 2nd number: '))
c = int(input('Enter 3st number: '))
d = int(input('Enter 4nd number: '))
print(minimize(a,b,c,d))
